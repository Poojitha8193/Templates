          }
        }
      },
      {
        "name": "sqlDeployment",
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2021-04-01",
        "properties": {
          "mode": "Incremental",
          "location": { "value": "East US 2" },
          "templateLink": {
            "uri": "https://github.com/Poojitha8193/Templates/sqlDatabaseModule.json",
            "contentVersion": "1.0.0.0"
          }
         
        }
      },
      {
        "name": "vmDeployment",
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2021-04-01",
        "properties": {
          "mode": "Incremental",
          "location": { "value": "East US 2" },
          "templateLink": {
            "uri": "https://github.com/Poojitha8193/Templates/vmModule.json",
